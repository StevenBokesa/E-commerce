<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>HundredTracks</display-name>
    <servlet>
	    <servlet-name>CommentServlet</servlet-name>
	    <servlet-class>controller.CommentServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>CommentPageServlet</servlet-name>
	    <servlet-class>controller.CommentPageServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>IndexServlet</servlet-name>
	    <servlet-class>controller.IndexServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>LoginServlet</servlet-name>
	    <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>HomeServlet</servlet-name>
    	<servlet-class>controller.HomeServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>HomeServlet2</servlet-name>
    	<servlet-class>controller.HomeServlet2</servlet-class>
    </servlet>
  
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controller.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>controller.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateEvent</servlet-name>
        <servlet-class>controller.CreateEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeProfile</servlet-name>
        <servlet-class>controller.ChangeProfile</servlet-class>
    </servlet>

  <servlet-mapping>
    <servlet-name>CommentServlet</servlet-name>
    <url-pattern>/comment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CommentPageServlet</servlet-name>
    <url-pattern>/commentPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>HomeServlet</servlet-name> 
    	<url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>HomeServlet2</servlet-name> 
    	<url-pattern>/home2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateEvent</servlet-name>
        <url-pattern>/CreateEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeProfile</servlet-name>
        <url-pattern>/ChangeProfile</url-pattern>
    </servlet-mapping>
    
</web-app>